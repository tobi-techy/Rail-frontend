{
  "name": "rail",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "jail-monkey"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "node ./node_modules/.bin/expo run:ios",
    "android": "node ./node_modules/.bin/expo run:android",
    "prebuild": "node ./node_modules/.bin/expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "web": "expo start --web",
    "docker:build": "docker build -t rail-mobile .",
    "docker:dev": "docker-compose up app-dev",
    "docker:prod": "docker-compose --profile production up",
    "docker:backend": "docker-compose --profile backend up",
    "docker:database": "docker-compose --profile database up postgres",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "setup:dev": "bun install && bun run prepare",
    "clean": "bun run clean:cache && bun run clean:deps",
    "clean:cache": "rm -rf .expo node_modules/.cache",
    "clean:deps": "rm -rf node_modules bun.lockb && bun install",
    "test:docker": "docker-compose exec app-dev bun test",
    "typecheck": "tsc --noEmit",
    "validate": "bun run typecheck && bun run lint && bun run test",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/ui": "~0.2.0-beta.9",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.8",
    "@sentry/react-native": "~7.2.0",
    "@shopify/react-native-skia": "2.2.12",
    "@sumsub/react-native-mobilesdk-module": "^1.40.2",
    "@tanstack/react-query": "^5.90.5",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@zamplyy/react-native-nice-avatar": "^1.1.2",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "expo": "~54.0.33",
    "expo-apple-authentication": "^8.0.8",
    "expo-application": "~7.0.8",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-localization": "~17.0.8",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-video": "~3.0.16",
    "expo-web-browser": "~15.0.10",
    "jail-monkey": "^2.8.0",
    "lucide-react-native": "^0.544.0",
    "moti": "^0.30.0",
    "nativewind": "^4.0.1",
    "posthog-react-native": "^4.35.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gleapsdk": "^15.2.1",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-otp-entry": "^1.8.5",
    "react-native-passkey": "3.3.2",
    "react-native-qrcode-skia": "^0.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "ws": "^8.16.0",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/qrcode": "^1.5.6",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-qrcode-styled": "^0.4.0",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/~/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/~/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|@sentry/react-native|sentry-expo|native-base|react-native-svg|moti|react-native-gifted-charts|gifted-charts-core|react-native-reanimated|lucide-react-native)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/~/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "private": true
}
