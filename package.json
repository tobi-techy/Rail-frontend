{
  "name": "stacks",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "docker:build": "docker build -t stacks-mobile .",
    "docker:dev": "docker-compose up app-dev",
    "docker:prod": "docker-compose --profile production up",
    "docker:backend": "docker-compose --profile backend up",
    "docker:database": "docker-compose --profile database up postgres",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "setup:dev": "npm install",
    "test:docker": "docker-compose exec app-dev npm test"
  },
  "dependencies": {
    "@expo/ui": "0.2.0-beta.7",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.6",
    "@rneui/base": "4.0.0-rc.8",
    "@rneui/themed": "4.0.0-rc.8",
    "@shopify/react-native-skia": "^2.2.12",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "expo": "~54.0.17",
    "expo-blur": "^15.0.7",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.16",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "^17.0.7",
    "expo-notifications": "^0.32.12",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "lucide-react-native": "^0.544.0",
    "moti": "^0.30.0",
    "nativewind": "4.1.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-otp-entry": "^1.8.5",
    "react-native-qrcode-skia": "^0.3.1",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "0.5.1",
    "ws": "^8.16.0",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.17",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.3"
  },
  "private": true
}
