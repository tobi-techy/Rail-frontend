# Stacks Mobile App - Development Environment (Using Shared Services)
# This configuration uses the existing PostgreSQL from stack_service project
# Switch between this and .env.development based on your needs

# Backend API Configuration
API_BASE_URL=http://localhost:3000/api
API_TIMEOUT=30000

# Database Configuration (Uses existing PostgreSQL on port 5432)
POSTGRES_DB=stacks
POSTGRES_USER=stacks_user
POSTGRES_PASSWORD=stacks_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Expo Configuration
EXPO_DEVTOOLS_LISTEN_ADDRESS=0.0.0.0
EXPO_PACKAGER_PROXY_URL=http://localhost:8081

# Development Settings
NODE_ENV=development
DEBUG=true
LOG_LEVEL=debug

# Cache Configuration
CACHE_TTL=3600
CACHE_PREFIX=stacks:dev:

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=false
ENABLE_DEBUG_TOOLS=true

# Security (Development Only)
JWT_SECRET=dev_jwt_secret_change_in_production
ENCRYPTION_KEY=dev_encryption_key_32_chars_long
BCRYPT_ROUNDS=8