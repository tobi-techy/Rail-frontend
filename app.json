{
  "expo": {
    "name": "Rail Money",
    "slug": "rail",
    "version": "1.0.5",
    "scheme": "rail",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-secure-store",
      "expo-local-authentication",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "organization": "rail-money",
          "project": "react-native"
        }
      ],
      "expo-web-browser",
      "expo-video",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/app-icon/app-icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "backgroundColor": "#FF5A00"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.railmoney.rail",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "Use Face ID to authenticate and secure your account",
        "NSCameraUsageDescription": "Camera access is required for QR code scanning",
        "UIRequiredDeviceCapabilities": ["armv7"],
        "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"],
        "UIStatusBarStyle": "UIStatusBarStyleDefault"
      },
      "icon": "./assets/app-icon/app-icon.png",
      "appleTeamId": "A239A6TG6R"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon/logo-dark.png",
        "backgroundColor": "#FF5A00"
      },
      "package": "com.railmoney.rail",
      "permissions": ["android.permission.USE_BIOMETRIC", "android.permission.USE_FINGERPRINT"]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8623a74b-7fb3-4d57-a2d6-b3fdef785ff0"
      }
    }
  }
}
