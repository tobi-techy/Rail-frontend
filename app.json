{
  "expo": {
    "name": "Rail Money",
    "slug": "rail",
    "version": "1.0.6",
    "scheme": "rail",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-secure-store",
      "expo-local-authentication",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FF2E01",
          "image": "./assets/app-icon/logo-transparent.png",
          "imageWidth": 150,
          "resizeMode": "contain"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "rail-money",
          "project": "react-native"
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "colorAccent": {
              "light": "#FF2E01",
              "dark": "#FF2E01"
            }
          }
        }
      ],
      "expo-web-browser",
      "expo-video",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/app-icon/app-icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/app-icon/logo-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF2E01"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2",
      "bundleIdentifier": "com.railmoney.rail",
      "splash": {
        "image": "./assets/app-icon/logo-transparent.png",
        "resizeMode": "contain",
        "backgroundColor": "#FF2E01"
      },
      "associatedDomains": ["webcredentials:api.userail.money", "webcredentials:userail.money"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "Use Face ID to authenticate and secure your account",
        "NSCameraUsageDescription": "Camera access is required for QR code scanning",
        "NSPhotoLibraryUsageDescription": "Access your photo library to upload profile pictures and payment verification documents",
        "NSLocationWhenInUseUsageDescription": "Your location helps us provide location-based services and improve your experience",
        "UIRequiredDeviceCapabilities": ["armv7"],
        "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"],
        "UIStatusBarStyle": "UIStatusBarStyleDefault"
      },
      "icon": "./assets/app-icon/app-icon.png",
      "appleTeamId": "A239A6TG6R"
    },
    "android": {
      "icon": "./assets/app-icon/app-icon.png",
      "splash": {
        "image": "./assets/app-icon/logo-transparent.png",
        "resizeMode": "contain",
        "backgroundColor": "#FF2E01"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon/app-icon.png",
        "backgroundColor": "#FF2E01"
      },
      "package": "com.railmoney.rail",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8623a74b-7fb3-4d57-a2d6-b3fdef785ff0"
      }
    }
  }
}
