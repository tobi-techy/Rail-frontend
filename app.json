{
  "expo": {
    "name": "rail",
    "slug": "rail",
    "version": "1.0.0",
    "scheme": "rail",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-secure-store",
      "expo-local-authentication",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "organization": "YOUR_SENTRY_ORG",
          "project": "YOUR_SENTRY_PROJECT"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/app-icon/ios-light.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.railfinance.rail",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "Use Face ID to authenticate and secure your account",
        "NSCameraUsageDescription": "Camera access is required for QR code scanning"
      },
      "icon": {
        "dark": "./assets/app-icon/ios-dark.png",
        "light": "./assets/app-icon/ios-light.png",
        "tinted": "./assets/app-icon/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.railfinance.rail",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2824fee-1afc-48e0-a253-e72399dd968b"
      }
    }
  }
}
